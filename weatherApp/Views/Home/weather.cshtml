
@{
    ViewBag.Title = "weather";
}

<h2>weather</h2>
<p id="name"></p>
<p id="temp"></p>
<p id="humidity"></p>
<button>Get Weather</button>
<script>
    $(document).ready(function () {
        $("button").click(function () {
            $.get("http://api.openweathermap.org/data/2.5/forecast/hourly?q=(Washington),us&appid=b6907d289e10d714a6e88b30761fae22", function (response) {
                //response
                
                console.log(response);
                $("#name").text(response.name);
                $("#temp").text(response.main.temp);
                $("#humidity").text(response.main.humidity);
            });
        });
    });

 </script>
